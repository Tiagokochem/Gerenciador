@model PlanoModel

@{
    ViewData["Title"] = "Confirmação de exclusão";
}

@*//colocar autorized
*@
<div class="text-center">
    <h3>Deseja realmente apagar o contato (@Model.ParceiroNome)</h3>
    </div>
    
<a class="btn btn-primary btn-block" role="button" asp-controller="Listar" asp-action="Index">Cancelar exclusão</a>
<a class="btn btn-danger btn-block" asp-action="Apagar" asp-controller="Listar" asp-route-id="@Model.Id" role="button">Confirmar exclusão</a>
