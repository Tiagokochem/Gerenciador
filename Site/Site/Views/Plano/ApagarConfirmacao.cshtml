@model PlanoModel
@{
    ViewData["Title"] = "Confirmação apagar contato";
}

<div class="text-center">
    <h3>Realmente deseja apagar o plano (@Model.Plano)?</h3>
</div>

<a class="btn btn-primary btn-block" asp-action="Index" asp-controller="Plano" role="button">Cancelar exclusão</a>
<a class="btn btn-danger btn-block" asp-action="Apagar" asp-controller="Plano" asp-route-id="@Model.Id" role="button">Confirmar exclusão</a>
